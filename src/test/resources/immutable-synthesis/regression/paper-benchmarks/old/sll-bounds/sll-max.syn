void sll_max (loc x, loc ret) {
  if (x == 0) {
    *ret = 0;
  } else {
    let v2 = *x;
    let nxt2 = *(x + 1);
    sll_max(nxt2, ret);
    let hi12 = *ret;
    *ret = hi12 <= v2 ? v2 : hi12;
  }
}