void list_copy (loc r, loc x) {
  if (x == 0) {
    *r = 0;
  } else {
    let v = *x;
    let n = *(x + 1);
    list_copy(x, n);
    let yx = *x;
    let y = malloc(2);
    *r = y;
    *(y + 1) = yx;
    *x = v;
    *y = v;
  }
}