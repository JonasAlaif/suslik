# -x true -p true
Deallocate two trees

###

{true; tree(x, s1)<a> ** tree(y, s2)<b>}
  void treefree2(loc x, loc y)
{true ; emp }

###

{tree(x, s1)<a> ** tree(y, s2)<b>}
{emp}
void treefree2 (loc x, loc y) {
  if (x == 0) {
    if (y == 0) {
    } else {
      let l = *(y + 1);
      let r = *(y + 2);
      treefree2(r, l);
      free(y);
    }
  } else {
    let lx = *(x + 1);
    let rx = *(x + 2);
    if (y == 0) {
      treefree2(rx, lx);
      free(x);
    } else {
      let l = *(y + 1);
      let r = *(y + 2);
      treefree2(r, l);
      treefree2(lx, rx);
      free(x);
      free(y);
    }
  }
}

