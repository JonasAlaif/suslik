void listfree2 (loc x, loc y) {
  if (x == 0) {
    listfree200(x, y);
  } else {
    let nxt2 = *x;
    listfree2(y, nxt2);
    free(x);
  }
}

void listfree200 (loc x, loc y) {
  if (y == 0) {
  } else {
    let nxt2 = *y;
    listfree200(x, nxt2);
    free(y);
  }
}

