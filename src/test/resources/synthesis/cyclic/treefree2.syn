# -x true
Deallocate two trees

###

{true; tree(x) ** tree(y)}
  void treefree2(loc x, loc y)
{true ; emp }

###

void treefree2 (loc x, loc y) {
  if (x == 0) {
    if (y == 0) {
    } else {
      let l = *y;
      let r = *(y + 1);
      treefree2(r, l);
      free(y);
    }
  } else {
    let lx = *x;
    let rx = *(x + 1);
    treefree2(rx, lx);
    if (y == 0) {
      free(x);
    } else {
      let l = *y;
      let r = *(y + 1);
      treefree2(r, l);
      free(x);
      free(y);
    }
  }
}

