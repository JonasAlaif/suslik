void bst_insert (loc x, loc r) {
  let k2 = *r;
  if (x == 0) {
    let y2 = malloc(3);
    *r = y2;
    *(y2 + 1) = 0;
    *(y2 + 2) = 0;
    *y2 = k2;
  } else {
    let v2 = *x;
    if (k2 <= v2) {
      let l2 = *(x + 1);
      bst_insert(l2, r);
      let y12 = *r;
      *(x + 1) = y12;
      *r = x;
    } else {
      let r12 = *(x + 2);
      bst_insert(r12, r);
      let y12 = *r;
      *(x + 2) = y12;
      *r = x;
    }
  }
}