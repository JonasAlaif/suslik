#####

void srtl_insert (loc x, loc r) {
  let k2 = *r;
  if (x == 0) {
    let y2 = malloc(2);
    *r = y2;
    *y2 = k2 <= 7 ? k2 : 7;
    *(y2 + 1) = 0;
  } else {
    let lo2 = *x;
    let nxt2 = *(x + 1);
    srtl_insert(nxt2, r);
    let y12 = *r;
    *x = k2 <= lo2 ? k2 : lo2;
    *(x + 1) = y12;
    *r = x;
  }
}