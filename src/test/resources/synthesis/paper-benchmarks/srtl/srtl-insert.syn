{0 <= n /\ 0 <= k /\ k <= 7 /\ (k < lo) ; r :-> z ** z :-> k ** (z + 1) :-> 0 ** [z, 2] ** srtl(x, n, lo, hi) }
void srtl_insert (loc x, loc r)
{n1 == n + 1 /\ lo1 == (k <= lo ? k : lo)  /\ hi1 == (hi <= k ? k : hi) ; r :-> y ** srtl(y, n1, lo1, hi1) }

#####

void srtl_insert (loc x, loc r) {
  let k2 = *r;
  if (x == 0) {
    let y2 = malloc(2);
    *r = y2;
    *(y2 + 1) = 0;
    *y2 = k2;
  } else {
    let v2 = *x;
    if (v2 <= k2) {
      let nxt2 = *(x + 1);
      srtl_insert(nxt2, r);
      let y12 = *r;
      *(x + 1) = y12;
      *r = x;
    } else {
      let nxt2 = *(x + 1);
      srtl_insert(nxt2, x);
      let y12 = *x;
      *r = x;
      *(x + 1) = y12;
      *x = k2;
    }
  }
}