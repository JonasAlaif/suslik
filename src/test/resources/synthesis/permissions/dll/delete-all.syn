# -b true -c 2
doubly-linked list: delete all occurrences of x

#####

{ dll(x, b, s, mut, mut, mut) ** ret :-> a }
void dll_delete_all (loc x, loc ret)
{ dll(y, c, s - {a}, mut, mut, mut) ** ret :-> y }

#####

void dll_delete_all (loc x, loc ret) {
  let a = *ret;
  if (x == 0) {
    *ret = 0;
  } else {
    let vx = *x;
    if (a <= vx && vx <= a) {
      let w = *(x + 1);
      dll_delete_all(w, ret);
      free(x);
    } else {
      let w = *(x + 1);
      dll_delete_all(w, ret);
      let y = *ret;
      if (y == 0) {
        *(x + 1) = 0;
        *ret = x;
      } else {
        let v = *y;
        if (vx <= v && v <= vx) {
          free(x);
        } else {
          *(y + 2) = x;
          *(x + 1) = y;
          *ret = x;
        }
      }
    }
  }
}

