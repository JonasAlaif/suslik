void listfree2 (loc x, loc y) {
  if (x == 0) {
    listfree200(x, y);
  } else {
    let n = *(x + 1);
    listfree2(y, n);
    free(x);
  }
}

void listfree200 (loc x, loc y) {
  if (y == 0) {
  } else {
    let n = *(y + 1);
    listfree200(x, n);
    free(y);
  }
}

